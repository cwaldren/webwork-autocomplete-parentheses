{
	"manifest_version": 2,
	"name": "WeBWorK Parentheses Autocompleter",
	"version": "1.0",
	"description": "Completes parentheses when you open them, also does some deleting.",
	"icons": { 
          "48": "48.png",
          "128": "128.png" 
     },
	"content_scripts": 
	[
		{
			"matches": ["*://*/webwork/*", "*://*/webwork2/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

